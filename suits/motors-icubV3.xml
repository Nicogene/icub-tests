<?xml version="1.0" encoding="UTF-8"?>

<suit name="Motor Control Suite">
    <description>Testing robot motors</description>
    <environment>--robotname icub --context icubV3</environment>
    <!--fixture param="--fixture icub-fixture.xml"> yarpmanager </fixture-->

    <!-- Motors -->
    <test type="dll" param="--from motortest_head.ini"> MotorTest </test> 

    <!-- Joint Limits -->
    <test type="dll" param="--from joint_limits_head.ini"> JointLimits </test>
    <test type="dll" param="--from joint_limits_face.ini"> JointLimits </test>
	
    <!-- Openloop -->
	<test type="dll" param="--from openLoopConsistency_head.ini"> OpenloopConsistency </test>
    <test type="dll" param="--from openLoopConsistency_face.ini"> OpenloopConsistency </test>
    
	
    <!-- Motor Stiction -->
    <test type="dll" param="--from motorStiction_head.ini"> MotorStiction </test>
    <test type="dll" param="--from motorStiction_face.ini"> MotorStiction </test> 

	
</suit>

